{
  extends: [
    "config:base",
  ],
  branchConcurrentLimit: 0,
  prConcurrentLimit: 0,
  prHourlyLimit: 0,
  regexManagers: [
    {
      fileMatch: [
        ".*\\.tf$",
      ],
      matchStrings: [
        "^ +source += +\"github\\.com/(?<depName>suzuki-shunsuke/test-terraform-module-renovate//modules/[^?]*)\\?ref=module_modules_.*_(?<currentValue>v\\d+\\.\\d+\\.\\d+(?:-\\d+)?)\"",
      ],
      packageNameTemplate: "suzuki-shunsuke/test-terraform-module-renovate",

      datasourceTemplate: "github-releases",
      // extract version from datasource (GitHub Releases)
      extractVersionTemplate: "^module_modules_.*_(?<version>v\\d+\\.\\d+\\.\\d+(?:-\\d+)?)$",
    },
    {
      fileMatch: [
        "^foo\\.txt$",
      ],
      matchStrings: [
        "foo: module_modules_.*_(?<currentValue>v\\d+\\.\\d+\\.\\d+(?:-\\d+)?)",
      ],
      packageNameTemplate: "suzuki-shunsuke/test-terraform-module-renovate",
      depNameTemplate: "suzuki-shunsuke/test-terraform-module-renovate//modules/foo",
      datasourceTemplate: "github-releases",
      // extract version from datasource (GitHub Releases)
      extractVersionTemplate: "^module_modules_.*_(?<version>v\\d+\\.\\d+\\.\\d+(?:-\\d+)?)$",
    },
  ],
}
